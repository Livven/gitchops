{
  "name": "sunnygit",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": ".",
  "scripts": {
    "start": "cross-env NODE_ENV=development NODE_OPTIONS=--max-old-space-size=4096 BROWSER=none TSC_COMPILE_ON_ERROR=true npm-run-all --parallel start:*",
    "start-built": "npm-run-all start:electron",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "package": "electron-builder --dir",
    "package-setup": "electron-builder --publish never",
    "lint": "npm-run-all --parallel lint:*",
    "format": "prettier --write .",
    "clean": "shx rm -rf {build,dist}",
    "pipeline": "npm-run-all clean lint build test package-setup",
    "---- lifecycle ----": "",
    "postinstall": "patch-package && electron-builder install-app-deps",
    "---- internal ----": "",
    "start:cra": "react-scripts start",
    "start:electron": "electron .",
    "lint:eslint": "cross-env ESLINT_IGNORE_TODOS=true eslint src --ext js,jsx,ts,tsx --max-warnings 0",
    "lint:stylelint": "stylelint \"src/**/*.{css,js,jsx,ts,tsx}\" --max-warnings 0",
    "lint:prettier": "prettier --check ."
  },
  "dependencies": {
    "@blueprintjs/core": "^3.28.0",
    "antd": "^4.2.5",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.8.27",
    "lodash": "^4.17.15",
    "monaco-editor": "^0.20.0",
    "nodegit": "^0.26.5",
    "react": "^16.13.1",
    "react-collapse": "^5.0.1",
    "react-dom": "^16.13.1",
    "react-measure": "^2.3.0",
    "react-monaco-editor": "^0.36.0",
    "react-window": "^1.8.5",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/crypto-js": "^3.1.47",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.152",
    "@types/node": "^12.12.42",
    "@types/nodegit": "^0.26.3",
    "@types/react": "^16.9.35",
    "@types/react-collapse": "^5.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-measure": "^2.0.6",
    "@types/react-window": "^1.8.2",
    "@types/styled-components": "^5.1.0",
    "cross-env": "^7.0.2",
    "electron": "^9.0.0",
    "electron-builder": "^22.6.1",
    "electron-devtools-installer": "^3.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-sort-destructure-keys": "^1.3.4",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "native-addon-loader": "^2.0.1",
    "node-loader": "^0.6.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.2.2",
    "prettier": "^2.0.5",
    "react-scripts": "3.4.1",
    "shx": "^0.3.2",
    "stylelint": "^13.5.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "type-fest": "^0.14.0",
    "typescript": "^3.9.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "endOfLine": "auto"
  }
}
