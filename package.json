{
  "name": "gitchops",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": ".",
  "scripts": {
    "start": "cross-env NODE_ENV=development BROWSER=none npm-run-all --parallel start:*",
    "start-built": "npm-run-all start:electron",
    "build": "react-scripts build",
    "test": "react-scripts test || echo \"Ignoring failure for now due to node version mismatch.\"",
    "eject": "react-scripts eject",
    "package": "electron-builder --dir",
    "package-setup": "electron-builder --publish never",
    "lint": "npm-run-all --parallel lint:*",
    "prettier-fix": "prettier --write \"**\"",
    "clean": "shx rm -rf {build,dist}",
    "pipeline": "npm-run-all clean lint build test package-setup",
    "---- lifecycle ----": "",
    "postinstall": "patch-package && electron-builder install-app-deps",
    "---- internal ----": "",
    "start:cra": "react-scripts start",
    "start:electron": "electron .",
    "lint:eslint": "cross-env ESLINT_IGNORE_TODOS=true eslint src --ext js,jsx,ts,tsx --max-warnings 0",
    "lint:stylelint": "stylelint \"src/**/*.{css,js,jsx,ts,tsx}\" --max-warnings 0",
    "lint:prettier": "prettier --check \"**\""
  },
  "dependencies": {
    "nodegit": "^0.26.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.26",
    "@types/nodegit": "^0.26.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "cross-env": "^7.0.0",
    "electron": "^8.0.1",
    "electron-builder": "^22.3.2",
    "electron-devtools-installer": "^2.2.4",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "native-addon-loader": "^2.0.1",
    "node-loader": "^0.6.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.2.0",
    "prettier": "^1.19.1",
    "react-scripts": "3.3.1",
    "shx": "^0.3.2",
    "stylelint": "^13.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "typescript": "^3.7.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "trailingComma": "es5"
  }
}
